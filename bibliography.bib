issn = {09646906},
journal = {Human Molecular Genetics},
number = {12},
pages = {2099--2107},
pmid = {9328474},
title = {{Higher proportion of intact exon 9 CFTR mRNA in nasal epithelium compared with vas deferens}},
volume = {6},
year = {1997}
}
