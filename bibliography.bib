doi = {10.1186/1472-6793-12-12},
file = {:C$\backslash$:/Users/kimyo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tucker et al. - 2012 - The $\Delta$F508-CFTR mutation inhibits wild-type CFTR processing and function when co-expressed in human airway epithel.pdf:pdf},
issn = {1472-6793},
