doi = {10.1371/journal.pbio.1000155},
file = {:C$\backslash$:/Users/kimyo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhang et al. - 2009 - CFTR delivery to 25{\%} of surface epithelial cells restores normal rates of mucus transport to human cystic fibrosis.pdf:pdf},
isbn = {1544-9173},
